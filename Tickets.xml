        <status>OK</status>
        <fareBasisLeft>2019-01-01</fareBasisLeft>
        <fareBasisRight>2019-01-01</fareBasisRight>
        <price>5000</price>
        <formOfPaint>INV</formOfPaint>
        <origin>MSK</origin>
        <destination>LED</destination>
        <passengerName>Andrew</passengerName>
        <passengerLastName>Rakitin</passengerLastName>
        <document>1168556575</document>
    </ticket>
</tickets>
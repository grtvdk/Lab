<!DOCTYPE tickets [
        <!ELEMENT tickets (ticket)*>
        <!ATTLIST tickets
                xmlns CDATA #REQUIRED
                xmlns:xsi CDATA #REQUIRED
                xsi:schemaLocation CDATA #REQUIRED>
        <!ELEMENT ticket (carrier|flightNum|placeNum|placeClass|date|time|status|fareBasisLeft|fareBasisRight|price|formOfPaint|origin|destination|passengerName|passengerLastName|document)*>
        <!ELEMENT carrier (#PCDATA)>
        <!ELEMENT flightNum (#PCDATA)>
        <!ELEMENT placeNum (#PCDATA)>
        <!ELEMENT placeClass (#PCDATA)>
        <!ELEMENT date (#PCDATA)>
        <!ELEMENT time (#PCDATA)>
        <!ELEMENT status (#PCDATA)>
        <!ELEMENT fareBasisLeft (#PCDATA)>
        <!ELEMENT fareBasisRight (#PCDATA)>
        <!ELEMENT price (#PCDATA)>
        <!ELEMENT formOfPaint (#PCDATA)>
        <!ELEMENT origin (#PCDATA)>
        <!ELEMENT destination (#PCDATA)>
        <!ELEMENT passengerName (#PCDATA)>
        <!ELEMENT passengerLastName (#PCDATA)>
        <!ELEMENT document (#PCDATA)>
        ]>
<tickets xmlns="https://www.w3schools.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://www.w3schools.com ./Tickets.xsd">
    <ticket>
        <carrier>SU</carrier>
